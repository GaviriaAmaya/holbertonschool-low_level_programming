#include "holberton.h"
/**
 * rev_string - Reverse a string without print it
 * @s: Pointer to the adress of the string
 *
 *Return: Void
 */

void rev_string(char *s)
{
  int t = 0;            
                      
  while (*(s + t))      
    t++;                  
  while (t--)           
    {                     
      *s;
    }                     
  _putchar ('\n');      
}
